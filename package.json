{
  "name": "stenciljs-storybook-env",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "custom-elements": "dist/custom-elements.json",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stenciljs-storybook-env/stenciljs-storybook-env.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "generate": "stencil generate",
    "test": "stencil test --spec",
    "e2e": "stencil test --e2e",
    "predev": "yarn install",
    "dev": "yarn stencil:build && concurrently 'yarn stencil:dev' 'yarn storybook:dev'",
    "prebuild": "yarn predev && rimraf loader dist docs",
    "build": "yarn stencil:build && yarn storybook:build",
    "stencil:dev": "stencil build --watch",
    "storybook:dev": "start-storybook -h 0.0.0.0 -p 80 -s dist",
    "stencil:build": "stencil build",
    "storybook:build": "build-storybook -o ./docs"
  },
  "devDependencies": {
    "@pxtrn/storybook-addon-docs-stencil": "^1.0.0",
    "@stencil/core": "^2.0.1",
    "@stencil/sass": "^1.4.1",
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/web-components": "^6.3.6",
    "@types/jest": "^26.0.24",
    "@types/puppeteer": "^5.4.3",
    "concurrently": "^6.2.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "lit-html": "^1.4.1",
    "puppeteer": "^5.5.0",
    "rimraf": "^3.0.2",
    "storybook": "^6.3.6",
    "typescript": "^4"
  },
  "license": "MIT"
}
